{
  "AWSEBDockerrunVersion": "3",
  "containerDefinitions": [
    {
      "name": "frontend",
      "image": "<your-docker-hub-username>/movie-icon-frontend:latest",
      "essential": true,
      "memory": 256,
      "portMappings": [
        {
          "hostPort": 80,
          "containerPort": 80
        }
      ],
      "links": ["backend"]
    },
    {
      "name": "backend",
      "image": "<your-docker-hub-username>/movie-icon-backend:latest",
      "essential": true,
      "memory": 256,
      "portMappings": [
        {
          "hostPort": 3000,
          "containerPort": 3000
        }
      ]
    }
  ]
} 